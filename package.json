{
  "name": "notebooklm-kit",
  "version": "2.1.1",
  "description": "TypeScript SDK for NotebookLM API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "postinstall": "npx playwright install chromium && npm run build",
    "setup": "npm install && npx playwright install chromium && npm run build",
    "install": "npm install",
    "build": "tsc",
    "build:dev": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "notebooklm",
    "google",
    "notebook",
    "ai",
    "research",
    "sdk",
    "typescript",
    "audio-overview",
    "podcast",
    "study-guide",
    "flashcards",
    "quiz",
    "mind-map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/photon-hq/notebooklm-kit"
  },
  "author": "photon-hq",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "playwright": "^1.57.0"
  }
}
